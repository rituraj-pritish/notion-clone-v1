{
  "extends": "next/core-web-vitals",
  "rules": {
    "indent": [
        "warn",
        "tab"
    ],
    "linebreak-style": [
        "warn",
        "unix"
    ],
    "quotes": [
        "warn",
        "single"
    ],
    "semi": [
        "warn",
        "always"
    ],
    "object-curly-spacing": ["warn", "always"],
    "no-multiple-empty-lines": ["error", { "max": 1 }]
}
}
